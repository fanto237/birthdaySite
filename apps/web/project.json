{
  "name": "web",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "root": "apps/web",
  "sourceRoot": "apps/web/src",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "npm run ng --prefix apps/web serve"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun --cwd apps/web ng build"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "bun --cwd apps/web ng test"
      }
    }
  },
  "tags": []
}
