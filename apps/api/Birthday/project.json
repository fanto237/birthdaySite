{
  "name": "api",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "root": "apps/api/Birthday",
  "sourceRoot": "apps/api/Birthday",
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet run --project apps/api/Birthday/Birthday.csproj"
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet build apps/api/Birthday/Birthday.csproj"
      }
    },
    "restore": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet restore apps/api/Birthday/Birthday.csproj"
      }
    },
    "publish": {
      "executor": "nx:run-commands",
      "options": {
        "command": "dotnet publish apps/api/Birthday/Birthday.csproj -c Release -o api /p:UseAppHost=false"
      }
    }
  },
  "tags": []
}
